# Configuración AWS para la aplicación de galería
# Duplica este archivo como .env.local y completa con tus valores reales

# Región de AWS
VITE_AWS_REGION=us-east-1

# Credenciales AWS (¡No uses credenciales reales en producción!)
# En producción usa IAM Roles o AWS Cognito
VITE_AWS_ACCESS_KEY_ID=tu_access_key_aqui
VITE_AWS_SECRET_ACCESS_KEY=tu_secret_key_aqui

# Nombre del bucket de S3
VITE_S3_BUCKET_NAME=gallery-photos-bucket

# Configuración adicional
VITE_MAX_FILE_SIZE_MB=10
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp
